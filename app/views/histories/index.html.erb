<p id="notice"><%= notice %></p>

<h1>Listing Histories</h1>

<table>
  <thead>
    <tr>
      <th>History</th>
      <th>Lec</th>
      <th>Bidang</th>
      <th>Unjuran</th>
      <th>Fac</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @histories.each do |history| %>
      <tr>
        <td><%= history.history_id %></td>
        <td><%= history.lec_id %></td>
        <td><%= history.bidang_id %></td>
        <td><%= history.unjuran_id %></td>
        <td><%= history.fac_id %></td>
        <td><%= history.year %></td>
        <td><%= link_to 'Show', history %></td>
        <td><%= link_to 'Edit', edit_history_path(history) %></td>
        <td><%= link_to 'Destroy', history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New History', new_history_path %>
